import java.util.Scanner;

public class H3Ej07 {

	public static void main(String[] args) {
		String a="";
		
		Scanner teclado = new Scanner(System.in);
		System.out.println("Escribe una frase ");
		a=teclado.next();
		
		while (a != ""){
			System.out.println("Escribe una frase ");
			a=teclado.next();
		}
	}
}
