
public class H3Ej19 {

	public static void main(String[] args) {
		
		int hora;
		int minuto;
		int segundo;
		
		for (segundo=0; segundo<61; segundo=segundo+1){
			if (segundo==60){
				segundo=0;
				minuto=minuto+1;
				if (minuto==60){
					minuto=0;
					hora=hora+1;
				}
			}
			System.out.println(hora + " : " + minuto + " : " + segundo );
		}
		
	}

}
