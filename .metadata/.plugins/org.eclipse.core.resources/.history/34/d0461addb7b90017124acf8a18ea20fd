import java.util.Scanner;

public class Strings06 {

	public static void main(String[] args) {
		
		String frase;
		final int max_length=80;
		String mayus="BCDFGHJKLMNÑPQRSTVWXYZ";
		String minus="AEIOU";
		String nums="0123456789";
		int pos=0;
		int contmin=0;
		int contmay=0;
		int contnum=0;
		
		Scanner teclado = new Scanner(System.in);
		do{
			System.out.println("La palabra debe tener como maximo " + max_length + " caracteres");
			frase=teclado.nextLine();
		}while (frase.length()>max_length);
		
		String mayusculas = frase.toUpperCase();
		
		for (int cont=0; cont<mayusculas.length(); cont++){
			char caracter = frase.charAt(pos);
			if(mayus.indexOf(caracter) != -1){
				contmay=contmay+1;
			}else if(minus.indexOf(caracter) != -1){
				contmin=contmin+1;
			}else{
				contnum=contnum+1;
			}
		}
		System.out.println("Tiene " + contnum + " numeros");			
		System.out.println("Tiene " + contmay + " mayusculas");			
		System.out.println("Tiene " + contmin + " minusculas");			
	}

}
